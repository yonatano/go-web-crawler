
# This file must be used with "source activate" or ". activate"

if [[ -n "${GOENV+1}" ]]; then
	deactivate
fi

export GOENV=go
export GOENV_OLDPS1=$PS1
export GOENV_OLDGOPATH=$GOPATH
export GOENV_OLDPATH=$PATH

export GOPATH=/Users/yonatanoren/.goenv/go
export PATH="$GOPATH/bin:$PATH"
export PS1="($(basename $GOPATH))$PS1"

mkdir -p $(dirname $GOPATH/src//Users/yonatanoren/Documents/Programming/go)
rm -f $GOPATH/src//Users/yonatanoren/Documents/Programming/go
ln -s /Users/yonatanoren/Documents/Programming/go $GOPATH/src//Users/yonatanoren/Documents/Programming/go

deactivate() {
	export PS1=$GOENV_OLDPS1
	export GOPATH=$GOENV_OLDGOPATH
	export PATH=$GOENV_OLDPATH

	unset GOENV GOENV_OLDPS1 GOENV_OLDPATH GOENV_OLDGOPATH
	unset -f deactivate
}
